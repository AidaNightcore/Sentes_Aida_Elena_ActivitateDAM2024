<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AdaugaAcvariu">

    <EditText
        android:id="@+id/etFirma"
        android:layout_width="208dp"
        android:layout_height="53dp"
        android:layout_marginStart="92dp"
        android:layout_marginTop="27dp"
        android:layout_marginEnd="63dp"
        android:layout_marginBottom="13dp"
        android:ems="10"
        android:inputType="text"
        android:text="Name"
        app:layout_constraintBottom_toTopOf="@+id/etGreutate"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/textView9"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/etPret"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="numberDecimal"
        tools:layout_editor_absoluteX="138dp"
        tools:layout_editor_absoluteY="225dp" />

    <EditText
        android:id="@+id/etGreutate"
        android:layout_width="206dp"
        android:layout_height="44dp"
        android:layout_marginStart="74dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="64dp"
        android:layout_marginBottom="12dp"
        android:ems="10"
        android:inputType="numberDecimal"
        app:layout_constraintBottom_toTopOf="@+id/spinnerCapacitate"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/etFirma" />

    <Spinner
        android:id="@+id/spinnerCapacitate"
        android:layout_width="207dp"
        android:layout_height="28dp"
        tools:layout_editor_absoluteX="141dp"
        tools:layout_editor_absoluteY="137dp" />

    <EditText
        android:id="@+id/etGrosimeSticla"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="number"
        tools:layout_editor_absoluteX="138dp"
        tools:layout_editor_absoluteY="172dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Greutate"
        tools:layout_editor_absoluteX="13dp"
        tools:layout_editor_absoluteY="95dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pret"
        tools:layout_editor_absoluteX="12dp"
        tools:layout_editor_absoluteY="233dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Capacitate"
        tools:layout_editor_absoluteX="13dp"
        tools:layout_editor_absoluteY="147dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Forma"
        tools:layout_editor_absoluteX="12dp"
        tools:layout_editor_absoluteY="42dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Grosime Sticla"
        tools:layout_editor_absoluteX="11dp"
        tools:layout_editor_absoluteY="190dp" />

    <Button
        android:id="@+id/btnAdaugare"
        android:layout_width="198dp"
        android:layout_height="63dp"
        android:text="Adauga Acvariu"
        tools:layout_editor_absoluteX="111dp"
        tools:layout_editor_absoluteY="310dp" />
</androidx.constraintlayout.widget.ConstraintLayout>