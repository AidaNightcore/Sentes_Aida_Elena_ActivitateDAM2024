<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AdaugareActivity">

    <Button
        android:id="@+id/btnAdaugare"
        android:layout_width="181dp"
        android:layout_height="43dp"
        android:layout_marginStart="115dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="115dp"
        android:layout_marginBottom="74dp"
        android:text="Adauga Acvariu"
        app:layout_constraintBottom_toTopOf="@+id/listAcvarii"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/etPret" />

    <LinearLayout
        android:id="@+id/listAcvarii"
        android:layout_width="409dp"
        android:layout_height="328dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="74dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="1dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnAdaugare" />

    <EditText
        android:id="@+id/etFirma"
        android:layout_width="204dp"
        android:layout_height="41dp"
        android:ems="10"
        android:inputType="text"
        android:text="Name"
        tools:layout_editor_absoluteX="144dp"
        tools:layout_editor_absoluteY="27dp" />

    <EditText
        android:id="@+id/etPret"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="numberDecimal"
        tools:layout_editor_absoluteX="138dp"
        tools:layout_editor_absoluteY="225dp" />

    <EditText
        android:id="@+id/etGreutate"
        android:layout_width="206dp"
        android:layout_height="44dp"
        android:ems="10"
        android:inputType="numberDecimal"
        tools:layout_editor_absoluteX="141dp"
        tools:layout_editor_absoluteY="81dp" />

    <Spinner
        android:id="@+id/spinnerCapacitate"
        android:layout_width="207dp"
        android:layout_height="28dp"
        tools:layout_editor_absoluteX="141dp"
        tools:layout_editor_absoluteY="137dp" />

    <EditText
        android:id="@+id/etGrosimeSticla"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="number"
        tools:layout_editor_absoluteX="138dp"
        tools:layout_editor_absoluteY="172dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Greutate"
        tools:layout_editor_absoluteX="13dp"
        tools:layout_editor_absoluteY="95dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pret"
        tools:layout_editor_absoluteX="12dp"
        tools:layout_editor_absoluteY="233dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Capacitate"
        tools:layout_editor_absoluteX="13dp"
        tools:layout_editor_absoluteY="147dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Forma"
        tools:layout_editor_absoluteX="12dp"
        tools:layout_editor_absoluteY="42dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Grosime Sticla"
        tools:layout_editor_absoluteX="11dp"
        tools:layout_editor_absoluteY="190dp" />
</androidx.constraintlayout.widget.ConstraintLayout>